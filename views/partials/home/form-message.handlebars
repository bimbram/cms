{{#if success_message}}
  <br/>
  <div class="alert alert-success">
    {{success_message}}
  </div>
{{/if}}

{{#if error_message}}
  <br/>
  <div class="alert alert-danger">
    {{error_message}}
  </div>
{{/if}}

{{#if error}}
  <br/>
  <div class="alert alert-danger">
    {{error}}
  </div>
{{/if}}
